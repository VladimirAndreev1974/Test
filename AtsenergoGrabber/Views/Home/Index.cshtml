@{
    ViewBag.Title = "Тестовое задание";
}

<div class="row">
    <form role="form" id="form">
        <div class="form-group">
            <label for="datepicker">Выберите дату:</label>
            <input type="text" class="form-control" id="datepicker" placeholder="" readonly="readonly" autocomplete="off">
        </div>

        <div class="form-group">
            <button type="button" class="btn btn-default" id="downloadBtn">Скачать</button>
            <button type="button" class="btn btn-default" id="loadBtn">Загрузить в БД</button>
            <button type="button" class="btn btn-default" id="displayBtn">Отобразить</button>
        </div>

        <div class="form-group" id="alert">
        </div>

    </form>
</div>

<div class="row" id="result"></div>

